<% @attractions.each do |a| %>
	Ride Name: <%= a.name %> <br>
	Nausea Rating: <%= a.nausea_rating %> <br>
	Happiness Rating: <%= a.happiness_rating %> <br>
	Minimum Height: <%= a.min_height %>
	Tickets: <%= a.tickets %> <br>

	<% if @user.admin %>
		 <%= link_to "Show #{a.name}", attraction_path(a)%>
	<% else %>
		<%= link_to "Go on #{a.name}", attraction_path(a) %>
	<% end %>
<% end %>

<% if @user.admin %>
	<%= link_to 'New Attraction', new_attraction_path %>
<% end %>